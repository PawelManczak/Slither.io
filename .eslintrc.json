{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["constants.js"],
    "rules": {
        "require-jsdoc": 0,
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}